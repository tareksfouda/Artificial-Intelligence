
public class test {
public static void main(String [] args){
	boolean[] test = new boolean[4];
	for (int i =0;i<4;i++){
		test[i]=false;
		System.out.print(test[i] + " ");
		
	}

	System.out.println("\n Now : ");

	boolean[] test2 = new boolean[4];
	int k = -1;
	for (int i = 0;i<4;i++){
		
		
		for(int j=i;j<4;j++){
           if(j==0) {
        	   for(int l= 0;l<4;l++){
        		   test2[l] = test[l];
        	   }
        	   for(int m = 0;m<4;m++){
        		   test2[m] = !test[m];
        	   }
        	   
        	   for(int q=0;q<4;q++){
        		   System.out.println(test2[q]);
        	   }
        	   
           }
		
		}

	}

	
}
}


